{
  "name": "task-manager-backend",
  "version": "0.1.0",
  "bin": {
    "task-manager-backend": "bin/task-manager-backend.js"
  },
  "scripts": {
    "build": "npm run build:cdk && npm run build:lambda",
    "build:cdk": "tsc",
    "build:lambda": "cd lambda && npm run build",
    "postinstall": "npm run install:lambda",
    "install:lambda": "cd lambda && npm install",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.170.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.170.0",
    "constructs": "^10.0.0"
  }
}
